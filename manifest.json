{
  "manifest_version": 2,
  "name": "Core notifier",
  "description": "This extensions checks for notifications in CORE smartwork.",
  "version": "1.6",
  "icons": {
    "16": "coreLogo.png",
    "48": "coreLogo.png",
    "128": "coreLogo.png"
  },
  "options_page": "options.html",
  "permissions": [
    "https://*/",
    "notifications"
  ],
  "browser_action": {
    "default_icon": "coreLogo.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["jquery-1.11.0.min.js", "defs.js", "background.js"]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}